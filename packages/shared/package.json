    "@trivago/prettier-plugin-sort-imports": "~5.2.2",
    "eslint": "~9.36.0",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "~3.6.2",
    "rimraf": "~6.0.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.45.0",
    "vitest": "~3.2.4"
