{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@8.6.7",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "lint": "eslint src/ --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0 --cache",
    "test:unit": "jest --config jest.config.ts",
    "test": "pnpm test:unit",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "preinstall": "npx only-allow pnpm",
    "## needed by @codegouvfr/react-dsfr": "//",
    "postinstall": "copy-dsfr-to-public",
    "predev": "only-include-used-icons",
    "prebuild": "only-include-used-icons"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.2.1",
    "@reduxjs/toolkit": "^2.0.1",
    "antd": "^5.13.2",
    "highcharts": "^11.3.0",
    "highcharts-react-official": "^3.2.1",
    "leaflet": "^1.9.4",
    "publicodes": "1.0.0-rc.7",
    "publicodes-rules": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-leaflet": "^4.2.1",
    "react-redux": "^9.1.0",
    "type-route": "^1.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.13",
    "@types/jest": "^29.5.11",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
