{
  "name": "web",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": "24",
    "pnpm": "10.13.1"
  },
  "scripts": {
    "setup-env-vars": "export $(cat .env | grep 'WEBAPP_' | xargs) && chmod +x scripts/generateEnvVarsJsFile.sh && bash scripts/generateEnvVarsJsFile.sh public/js/env-vars.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "lint": "eslint --max-warnings 0 .",
    "test:unit": "vitest --globals=true",
    "test": "pnpm test:unit",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "preinstall": "npx only-allow pnpm",
    "## needed by @codegouvfr/react-dsfr": "//",
    "postinstall": "copy-dsfr-to-public",
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "~1.26.0",
    "@headlessui/react": "^2.2.9",
    "@react-pdf/renderer": "^4.3.1",
    "@reduxjs/toolkit": "~2.8.2",
    "clsx": "~2.1.1",
    "date-fns": "~4.1.0",
    "highcharts": "~12.2.0",
    "highcharts-react-official": "~3.2.2",
    "react": "~19.1.1",
    "react-dom": "~19.1.1",
    "react-hook-form": "~7.61.1",
    "react-pdf-tailwind": "^3.0.0",
    "react-redux": "~9.2.0",
    "reduce-reducers": "~1.0.4",
    "shared": "workspace:*",
    "type-route": "~1.1.0",
    "uuid": "~11.1.0",
    "zod": "~4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "~9.32.0",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "~5.2.2",
    "@types/geojson": "~7946.0.16",
    "@types/react": "~19.1.15",
    "@types/react-dom": "~19.1.9",
    "@types/uuid": "~10.0.0",
    "@vitejs/plugin-react": "~4.7.0",
    "eslint": "~9.32.0",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "~7.37.5",
    "eslint-plugin-react-hooks": "~5.2.0",
    "eslint-plugin-react-refresh": "~0.4.22",
    "globals": "~16.3.0",
    "jsdom": "~26.1.0",
    "prettier": "~3.6.2",
    "tailwindcss": "~4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.38.0",
    "vite": "~7.1.7",
    "vitest": "~3.2.4"
  }
}
