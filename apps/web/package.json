{
  "name": "web",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.13.0",
  "engines": {
    "node": "20",
    "pnpm": "9.13.0"
  },
  "scripts": {
    "setup-env-vars": "export $(cat .env | grep 'WEBAPP_' | xargs) && chmod +x scripts/generateEnvVarsJsFile.sh && bash scripts/generateEnvVarsJsFile.sh public/js/env-vars.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "lint": "eslint --max-warnings 0 .",
    "test:unit": "vitest --globals=true",
    "test": "pnpm test:unit",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "preinstall": "npx only-allow pnpm",
    "## needed by @codegouvfr/react-dsfr": "//",
    "postinstall": "copy-dsfr-to-public",
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "~1.13.9",
    "@reactour/tour": "~3.7.0",
    "@reduxjs/toolkit": "~2.2.8",
    "antd": "~5.21.6",
    "clsx": "~2.1.1",
    "date-fns": "~3.6.0",
    "highcharts": "~11.4.8",
    "highcharts-react-official": "~3.2.1",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "react-hook-form": "~7.53.2",
    "react-redux": "~9.1.2",
    "reduce-reducers": "~1.0.4",
    "shared": "workspace:*",
    "type-route": "~1.1.0",
    "uuid": "~10.0.0",
    "zod": "~3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "~1.1.1",
    "@eslint/js": "~9.11.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@trivago/prettier-plugin-sort-imports": "~4.3.0",
    "@types/eslint__js": "~8.42.3",
    "@types/geojson": "~7946.0.14",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.1",
    "@types/uuid": "~10.0.0",
    "@vitejs/plugin-react": "~4.3.3",
    "autoprefixer": "~10.4.20",
    "eslint": "~9.11.1",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-react": "~7.37.2",
    "eslint-plugin-react-hooks": "~4.6.2",
    "eslint-plugin-react-refresh": "~0.4.14",
    "globals": "~15.9.0",
    "jsdom": "~25.0.1",
    "postcss": "~8.4.49",
    "prettier": "~3.3.3",
    "tailwindcss": "~3.4.14",
    "typescript": "~5.5.4",
    "typescript-eslint": "~8.7.0",
    "vite": "~5.4.11",
    "vitest": "~2.1.5"
  }
}
