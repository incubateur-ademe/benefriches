{
  "name": "web",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.13.0",
  "engines": {
    "node": "20",
    "pnpm": "9.13.0"
  },
  "scripts": {
    "setup-env-vars": "export $(cat .env | grep 'WEBAPP_' | xargs) && chmod +x scripts/generateEnvVarsJsFile.sh && bash scripts/generateEnvVarsJsFile.sh public/js/env-vars.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "lint": "eslint --max-warnings 0 .",
    "test:unit": "vitest --globals=true",
    "test": "pnpm test:unit",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "preinstall": "npx only-allow pnpm",
    "## needed by @codegouvfr/react-dsfr": "//",
    "postinstall": "copy-dsfr-to-public",
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "~1.16.9",
    "@reactour/tour": "~3.7.0",
    "@reduxjs/toolkit": "~2.5.0",
    "antd": "~5.23.2",
    "clsx": "~2.1.1",
    "date-fns": "~4.1.0",
    "highcharts": "~12.1.2",
    "highcharts-react-official": "~3.2.1",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-hook-form": "~7.54.2",
    "react-redux": "~9.2.0",
    "reduce-reducers": "~1.0.4",
    "shared": "workspace:*",
    "type-route": "~1.1.0",
    "uuid": "~11.0.5",
    "zod": "~3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "~9.18.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@trivago/prettier-plugin-sort-imports": "~5.2.1",
    "@types/eslint__js": "~8.42.3",
    "@types/geojson": "~7946.0.15",
    "@types/react": "~19.0.7",
    "@types/react-dom": "~19.0.3",
    "@types/uuid": "~10.0.0",
    "@vitejs/plugin-react": "~4.3.4",
    "autoprefixer": "~10.4.20",
    "eslint": "~9.18.0",
    "eslint-config-prettier": "~10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react": "~7.37.4",
    "eslint-plugin-react-hooks": "~5.1.0",
    "eslint-plugin-react-refresh": "~0.4.18",
    "globals": "~15.14.0",
    "jsdom": "~26.0.0",
    "postcss": "~8.5.1",
    "prettier": "~3.4.2",
    "tailwindcss": "~3.4.17",
    "typescript": "~5.7.3",
    "typescript-eslint": "~8.21.0",
    "vite": "~6.0.11",
    "vitest": "~3.0.3"
  }
}
