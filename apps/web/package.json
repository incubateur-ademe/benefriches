{
  "name": "web",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": "20",
    "pnpm": "9.1.1"
  },
  "scripts": {
    "setup-env-vars": "export $(cat .env | grep 'WEBAPP_' | xargs) && chmod +x scripts/generateEnvVarsJsFile.sh && bash scripts/generateEnvVarsJsFile.sh public/js/env-vars.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "lint": "eslint src/ --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 1 --cache",
    "test:unit": "jest --config jest.config.ts",
    "test": "pnpm test:unit",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "preinstall": "npx only-allow pnpm",
    "## needed by @codegouvfr/react-dsfr": "//",
    "postinstall": "copy-dsfr-to-public",
    "predev": "only-include-used-icons",
    "prebuild": "only-include-used-icons"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.9.17",
    "@reduxjs/toolkit": "^2.2.5",
    "antd": "^5.18.1",
    "clsx": "^2.1.1",
    "highcharts": "^11.4.3",
    "highcharts-react-official": "^3.2.1",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "react-leaflet": "^4.2.1",
    "react-redux": "^9.1.2",
    "shared": "workspace:*",
    "type-route": "^1.1.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.14",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.9.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  }
}
