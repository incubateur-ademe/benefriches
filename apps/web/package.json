{
  "name": "web",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": "24",
    "pnpm": "10.24.0"
  },
  "scripts": {
    "setup-env-vars": "export $(cat .env | grep 'WEBAPP_' | xargs) && chmod +x scripts/generateEnvVarsJsFile.sh && bash scripts/generateEnvVarsJsFile.sh public/js/env-vars.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "lint": "oxlint --type-aware",
    "test:unit": "vitest --globals=true",
    "test": "pnpm test:unit",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "preinstall": "npx only-allow pnpm",
    "## needed by @codegouvfr/react-dsfr": "//",
    "postinstall": "copy-dsfr-to-public",
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "~1.31.0",
    "@headlessui/react": "^2.2.6",
    "@react-pdf/renderer": "^4.3.2",
    "@reduxjs/toolkit": "~2.11.2",
    "clsx": "~2.1.1",
    "date-fns": "~4.1.0",
    "highcharts": "12.2.0",
    "highcharts-react-official": "3.2.2",
    "react": "~19.2.3",
    "react-dom": "~19.2.3",
    "react-hook-form": "~7.69.0",
    "react-pdf-tailwind": "^3.0.0",
    "react-redux": "~9.2.0",
    "reduce-reducers": "~1.0.4",
    "shared": "workspace:*",
    "type-route": "~1.1.0",
    "uuid": "~13.0.0",
    "zod": "~4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@trivago/prettier-plugin-sort-imports": "~6.0.0",
    "@types/geojson": "~7946.0.16",
    "@types/react": "~19.2.7",
    "@types/react-dom": "~19.2.3",
    "@vitejs/plugin-react": "~5.1.2",
    "globals": "~16.5.0",
    "jsdom": "~27.4.0",
    "tailwindcss": "~4.1.18",
    "vite": "~7.3.0",
    "vite-plugin-node-polyfills": "~0.24.0"
  }
}
