{
  "name": "web",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": "20",
    "pnpm": "9.1.1"
  },
  "scripts": {
    "setup-env-vars": "export $(cat .env | grep 'WEBAPP_' | xargs) && chmod +x scripts/generateEnvVarsJsFile.sh && bash scripts/generateEnvVarsJsFile.sh public/js/env-vars.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/",
    "lint": "eslint --max-warnings 0 .",
    "test:unit": "vitest --globals=true",
    "test": "pnpm test:unit",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "preinstall": "npx only-allow pnpm",
    "## needed by @codegouvfr/react-dsfr": "//",
    "postinstall": "copy-dsfr-to-public",
    "predev": "only-include-used-icons",
    "prebuild": "only-include-used-icons"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.9.17",
    "@reduxjs/toolkit": "^2.2.5",
    "antd": "^5.18.1",
    "clsx": "^2.1.1",
    "highcharts": "^11.4.3",
    "highcharts-react-official": "^3.2.1",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "react-leaflet": "^4.2.1",
    "react-redux": "^9.1.2",
    "shared": "workspace:*",
    "type-route": "^1.1.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.5.0",
    "@types/eslint__js": "^8.42.3",
    "@types/geojson": "^7946.0.14",
    "@types/leaflet": "^1.9.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "globals": "^15.6.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}
