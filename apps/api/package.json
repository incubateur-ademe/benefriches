{
  "name": "api",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "pnpm@9.13.0",
  "engines": {
    "node": "22",
    "pnpm": "9.13.0"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "jest --config ./test/jest.config.ts",
    "test:integration": "jest --runInBand --config ./test/jest-integration.config.ts",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "knex:new-migration": "knex migrate:make -x ts --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:migrate-latest": "knex migrate:latest --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:migrate-rollback": "knex migrate:rollback --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-make": "knex seed:make --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-run": "knex seed:run --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts"
  },
  "devDependencies": {
    "@eslint/js": "~9.18.0",
    "@nestjs/cli": "~11.0.2",
    "@nestjs/schematics": "~11.0.0",
    "@nestjs/testing": "~11.0.5",
    "@trivago/prettier-plugin-sort-imports": "~5.2.1",
    "@types/bcrypt": "~5.0.2",
    "@types/express": "~5.0.0",
    "@types/jest": "~29.5.14",
    "@types/node": "~22.10.10",
    "@types/pg": "~8.11.11",
    "@types/supertest": "~6.0.2",
    "@types/uuid": "~10.0.0",
    "dotenv": "~16.4.7",
    "eslint": "~9.18.0",
    "eslint-config-prettier": "~10.0.1",
    "eslint-plugin-jest": "~28.11.0",
    "globals": "~15.14.0",
    "jest": "~29.7.0",
    "schematics@10.2.3": "link:nestjs/schematics@10.2.3",
    "supertest": "~7.0.0",
    "testcontainers": "~10.17.1",
    "ts-jest": "~29.2.5",
    "ts-node": "~10.9.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "~8.21.0"
  },
  "dependencies": {
    "@nestjs/axios": "~4.0.0",
    "@nestjs/common": "~11.0.5",
    "@nestjs/config": "~4.0.0",
    "@nestjs/core": "~11.0.5",
    "@nestjs/platform-express": "~11.0.5",
    "axios": "~1.7.9",
    "bcrypt": "~5.1.1",
    "date-fns": "~4.1.0",
    "knex": "~3.1.0",
    "nestjs-zod": "~4.3.1",
    "pg": "~8.13.1",
    "reflect-metadata": "~0.2.2",
    "rxjs": "~7.8.1",
    "shared": "workspace:^",
    "uuid": "~11.0.5",
    "zod": "~3.24.2"
  }
}
