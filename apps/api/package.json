{
  "name": "api",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": "24",
    "pnpm": "10.13.1"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "vitest --config test/vitest.config.ts",
    "test:integration": "vitest --config test/vitest.integration.config.ts",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "knex:new-migration": "knex migrate:make -x ts --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:migrate-latest": "knex migrate:latest --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:migrate-rollback": "knex migrate:rollback --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-make": "knex seed:make --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-run": "knex seed:run --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts"
  },
  "devDependencies": {
    "@eslint/js": "~9.32.0",
    "@nestjs/cli": "~11.0.8",
    "@nestjs/schematics": "~11.0.5",
    "@nestjs/testing": "~11.1.5",
    "@swc/core": "^1.13.3",
    "@trivago/prettier-plugin-sort-imports": "~5.2.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "~5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "~30.0.0",
    "@types/node": "~24.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "~8.15.5",
    "@types/supertest": "~6.0.3",
    "@types/uuid": "~10.0.0",
    "cookie": "^1.0.2",
    "dotenv": "~17.2.1",
    "eslint": "~9.32.0",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-jest": "~29.0.1",
    "eslint-plugin-n": "^17.21.3",
    "globals": "~16.3.0",
    "prettier": "~3.6.2",
    "supertest": "~7.1.4",
    "testcontainers": "~11.4.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.38.0",
    "unplugin-swc": "^1.5.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "~3.2.4"
  },
  "dependencies": {
    "@nestjs/axios": "~4.0.1",
    "@nestjs/common": "~11.1.5",
    "@nestjs/config": "~4.0.2",
    "@nestjs/core": "~11.1.5",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "~11.1.5",
    "axios": "~1.12.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "date-fns": "~4.1.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "knex": "~3.1.0",
    "nestjs-zod": "5.0.0-beta.20250719T005107",
    "nodemailer": "^7.0.5",
    "openid-client": "^6.6.2",
    "pg": "~8.16.3",
    "reflect-metadata": "~0.2.2",
    "rxjs": "~7.8.2",
    "shared": "workspace:^",
    "uuid": "~11.1.0",
    "zod": "~4.0.17"
  }
}
