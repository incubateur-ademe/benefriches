{
  "name": "api",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "pnpm@9.13.0",
  "engines": {
    "node": "22",
    "pnpm": "9.13.0"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "vitest --config test/vitest.config.ts",
    "test:integration": "vitest --config test/vitest.integration.config.ts",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "knex:new-migration": "knex migrate:make -x ts --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:migrate-latest": "knex migrate:latest --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:migrate-rollback": "knex migrate:rollback --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-make": "knex seed:make --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-run": "knex seed:run --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts"
  },
  "devDependencies": {
    "@eslint/js": "~9.28.0",
    "@nestjs/cli": "~11.0.7",
    "@nestjs/schematics": "~11.0.5",
    "@nestjs/testing": "~11.1.3",
    "@swc/core": "^1.11.31",
    "@trivago/prettier-plugin-sort-imports": "~5.2.2",
    "@types/express": "~5.0.3",
    "@types/jest": "~29.5.14",
    "@types/node": "~22.14.0",
    "@types/pg": "~8.15.4",
    "@types/supertest": "~6.0.3",
    "@types/uuid": "~10.0.0",
    "dotenv": "~16.5.0",
    "eslint": "~9.28.0",
    "eslint-config-prettier": "~10.1.5",
    "eslint-plugin-jest": "~28.13.0",
    "globals": "~16.2.0",
    "schematics@10.2.3": "link:nestjs/schematics@10.2.3",
    "supertest": "~7.1.1",
    "testcontainers": "~11.0.3",
    "ts-node": "~10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.34.0",
    "unplugin-swc": "^1.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "~3.2.3"
  },
  "dependencies": {
    "@nestjs/axios": "~4.0.0",
    "@nestjs/common": "~11.1.3",
    "@nestjs/config": "~4.0.2",
    "@nestjs/core": "~11.1.3",
    "@nestjs/platform-express": "~11.1.3",
    "axios": "~1.9.0",
    "date-fns": "~4.1.0",
    "knex": "~3.1.0",
    "nestjs-zod": "~4.3.1",
    "pg": "~8.16.0",
    "reflect-metadata": "~0.2.2",
    "rxjs": "~7.8.2",
    "shared": "workspace:^",
    "uuid": "~11.1.0",
    "zod": "~3.25.57"
  }
}
