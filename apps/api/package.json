{
  "name": "api",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": "24",
    "pnpm": "10.24.0"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "vitest --config test/vitest.config.ts",
    "test:integration": "vitest --config test/vitest.integration.config.ts",
    "lint": "oxlint --type-aware",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "knex:new-migration": "knex migrate:make -x ts --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:migrate-latest": "knex migrate:latest --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:migrate-latest:js": "knex migrate:latest --knexfile=dist/src/shared-kernel/adapters/sql-knex/knexfile.js",
    "knex:migrate-rollback": "knex migrate:rollback --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-make": "knex seed:make --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-run": "knex seed:run --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts"
  },
  "devDependencies": {
    "@nestjs/cli": "~11.0.14",
    "@nestjs/schematics": "~11.0.9",
    "@nestjs/testing": "~11.1.11",
    "@swc/core": "^1.15.8",
    "@trivago/prettier-plugin-sort-imports": "~6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "~5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/node": "~24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "~8.16.0",
    "@types/supertest": "~6.0.3",
    "cookie": "^1.1.1",
    "dotenv": "~17.2.3",
    "globals": "~16.5.0",
    "supertest": "~7.1.4",
    "testcontainers": "~11.11.0",
    "ts-node": "~10.9.2",
    "unplugin-swc": "^1.5.9",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/axios": "~4.0.1",
    "@nestjs/common": "~11.1.11",
    "@nestjs/config": "~4.0.2",
    "@nestjs/core": "~11.1.11",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-express": "~11.1.11",
    "axios": "~1.13.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "date-fns": "~4.1.0",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "knex": "~3.1.0",
    "nestjs-zod": "5.0.1",
    "nodemailer": "^7.0.12",
    "openid-client": "^6.8.1",
    "pg": "~8.16.3",
    "reflect-metadata": "~0.2.2",
    "rxjs": "~7.8.2",
    "shared": "workspace:^",
    "uuid": "~13.0.0",
    "zod": "~4.2.1"
  }
}
