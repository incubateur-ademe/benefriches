{
  "name": "api",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@8.6.7",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "jest --config ./test/jest.config.ts",
    "test:integration": "jest --runInBand --config ./test/jest-integration.config.ts",
    "lint": "eslint . --ext js,ts --report-unused-disable-directives --max-warnings 0 --cache",
    "format": "prettier --write --cache \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "format:check": "prettier --check --cache  \"./**/*.{js,jsx,ts,tsx,css,md,json}\" src/ test/",
    "knex:new-migration": "knex migrate:make -x ts --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:migrate-latest": "knex migrate:latest --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-make": "knex seed:make --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts",
    "knex:seed-run": "knex seed:run --knexfile=src/shared-kernel/adapters/sql-knex/knexfile.ts"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.6",
    "@types/passport-jwt": "^3.0.10",
    "@types/supertest": "^2.0.14",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "testcontainers": "^10.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "knex": "^2.5.1",
    "nestjs-zod": "^2.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
